  <div class="content">
    @RenderBody()
  </div>

  <footer class="footer">
    <div class="container">
      © @DateTime.Now.Year –
      <a href="https://karanlvm.info" target="_blank">
        Developed by Karan Vasudevamurthy
      </a>
    </div>
